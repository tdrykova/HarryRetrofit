<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MainFragment">

    <fragment
        android:id="@+id/MainFragment"
        android:name="com.tatry.harryretrofit.presentation.MainFragment"
        android:label="First Fragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_MainFragment_to_DbFragment"
            app:destination="@+id/DbFragment"
            />

    </fragment>

    <fragment
        android:id="@+id/DbFragment"
        android:name="com.tatry.harryretrofit.presentation.DbFragment"
        android:label="Second Fragment"
        tools:layout="@layout/fragment_db">
        <action android:id="@+id/action_DbFragment_to_MainFragment"
            app:destination="@+id/MainFragment"/>
    </fragment>
</navigation>